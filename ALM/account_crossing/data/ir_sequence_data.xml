<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Secuencia para Cruces Contables -->
        <record id="sequence_account_crossing" model="ir.sequence">
            <field name="name">Cruce Contable</field>
            <field name="code">account.crossing</field>
            <field name="prefix">CROSS/%(year)s/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>
    </data>
</odoo>