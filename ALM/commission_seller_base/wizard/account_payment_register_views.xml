<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_account_payment_register_form_commission" model="ir.ui.view">
        <field name="name">account.payment.register.form.commission</field>
        <field name="model">account.payment.register</field>
        <field name="inherit_id" ref="account.view_account_payment_register_form"/>
        <field name="arch" type="xml">
            <field name="payment_method_line_id" position="after">
                <field name="commission_seller_id"
                       context="{'search_commission_seller': True}"/>
            </field>
        </field>
    </record>
</odoo>